{
  "modno": 1,
  "version": 4,
  "instance": "demo-backrest",
  "repos": [
    {
      "id": "sample-repo",
      "uri": "s3://example-bucket/backups",
      "env": [
        "AWS_ACCESS_KEY_ID=EXAMPLEKEY",
        "AWS_SECRET_ACCESS_KEY=EXAMPLESECRET"
      ],
      "prunePolicy": {
        "schedule": {
          "cron": "0 0 * * *",
          "clock": "CLOCK_LAST_RUN_TIME"
        },
        "maxUnusedPercent": 10
      },
      "checkPolicy": {
        "schedule": {
          "cron": "0 0 1 * *",
          "clock": "CLOCK_LAST_RUN_TIME"
        },
        "readDataSubsetPercent": 0
      }
    }
  ],
  "plans": [
    
  ],
  "auth": {
    "users": [
      {
        "name": "demo",
        "passwordBcrypt": "$2a$10$exampleexampleexampleexampleexampleexampleexampleexample"
      }
    ]
  }
}
